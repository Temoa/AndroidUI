<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.temoa.androidui"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ToolbarActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".CustomToolbarActivity">
        </activity>
        <activity android:name=".FloatingActionButtonActivity">
        </activity>
        <activity android:name=".TextInputLayoutActivity">
        </activity>
        <activity android:name=".SnackbarActivity">
        </activity>
        <activity android:name=".TabLayoutActivity">
        </activity>
        <activity android:name=".CoordinatorLayoutActivity">
        </activity>
        <activity android:name=".CollapsingToolbarLayoutActivity">
        </activity>
        <activity android:name=".NavigationViewActivity">
        </activity>
        <activity android:name=".MaterialDialogActivity">
        </activity>
        <activity android:name=".SwipeRefreshLayoutActivity">
        </activity>
        <activity android:name=".LinearLayoutCompatActivity">
        </activity>
        <activity android:name=".PopupMenuActivity">
        </activity>
        <activity android:name=".other.ToImageActivity">
        </activity>
        <activity android:name=".FromImageActivity">
        </activity>
    </application>

</manifest>